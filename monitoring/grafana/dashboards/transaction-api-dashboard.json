{
  "id": null,
  "title": "Transaction API Dashboard",
  "tags": ["transaction-api", "tech-test", "spring-boot"],
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Transaction Submissions Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(transaction_submissions_total{job=\"tech-test-app\"}[5m])",
          "legendFormat": "Submissions/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 10},
              {"color": "red", "value": 50}
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Transaction Retrievals Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(transaction_retrievals_total{job=\"tech-test-app\"}[5m])",
          "legendFormat": "Retrievals/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 20},
              {"color": "red", "value": 100}
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 6,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "Error Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(transaction_errors_total{job=\"tech-test-app\"}[5m])",
          "legendFormat": "Errors/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1},
              {"color": "red", "value": 5}
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 4,
      "title": "Success Rate %",
      "type": "stat",
      "targets": [
        {
          "expr": "clamp_max((1 - (rate(transaction_errors_total{job=\"tech-test-app\"}[5m]) / (rate(transaction_submissions_total{job=\"tech-test-app\"}[5m]) + rate(transaction_retrievals_total{job=\"tech-test-app\"}[5m]) + 0.0001))) * 100, 100)",
          "legendFormat": "Success Rate"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 95},
              {"color": "green", "value": 99}
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 0
      }
    },
    {
      "id": 5,
      "title": "Success vs Error Distribution",
      "type": "piechart",
      "targets": [
        {
          "expr": "rate(transaction_submissions_total{job=\"tech-test-app\"}[5m]) + rate(transaction_retrievals_total{job=\"tech-test-app\"}[5m])",
          "legendFormat": "Success"
        },
        {
          "expr": "rate(transaction_errors_total{job=\"tech-test-app\"}[5m])",
          "legendFormat": "Errors"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 6,
      "title": "Error Rate Percentage",
      "type": "gauge",
      "targets": [
        {
          "expr": "clamp_max((rate(transaction_errors_total{job=\"tech-test-app\"}[5m]) / (rate(transaction_submissions_total{job=\"tech-test-app\"}[5m]) + rate(transaction_retrievals_total{job=\"tech-test-app\"}[5m]) + 0.0001)) * 100, 100)",
          "legendFormat": "Error Rate %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1},
              {"color": "red", "value": 5}
            ]
          },
          "min": 0,
          "max": 10
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      }
    },
    {
      "id": 7,
      "title": "Total Success vs Errors",
      "type": "stat",
      "targets": [
        {
          "expr": "transaction_submissions_total{job=\"tech-test-app\"} + transaction_retrievals_total{job=\"tech-test-app\"}",
          "legendFormat": "Total Success"
        },
        {
          "expr": "transaction_errors_total{job=\"tech-test-app\"}",
          "legendFormat": "Total Errors"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      }
    },
    {
      "id": 8,
      "title": "Success Rate Trend",
      "type": "timeseries",
      "targets": [
        {
          "expr": "clamp_max((1 - (rate(transaction_errors_total{job=\"tech-test-app\"}[5m]) / (rate(transaction_submissions_total{job=\"tech-test-app\"}[5m]) + rate(transaction_retrievals_total{job=\"tech-test-app\"}[5m]) + 0.0001))) * 100, 100)",
          "legendFormat": "Success Rate %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 95},
              {"color": "green", "value": 99}
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      }
    },
    {
      "id": 9,
      "title": "Error Rate Trend",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(transaction_errors_total{job=\"tech-test-app\"}[5m])",
          "legendFormat": "Error Rate"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1},
              {"color": "red", "value": 5}
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 24
      }
    },
    {
      "id": 11,
      "title": "HTTP Status Codes Over Time",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(http_server_requests_seconds_count{job=\"tech-test-app\",status=\"200\"}[5m])",
          "legendFormat": "200 OK - {{uri}}"
        },
        {
          "expr": "rate(http_server_requests_seconds_count{job=\"tech-test-app\",status=\"400\"}[5m])",
          "legendFormat": "400 Bad Request - {{uri}}"
        },
        {
          "expr": "rate(http_server_requests_seconds_count{job=\"tech-test-app\",status=\"404\"}[5m])",
          "legendFormat": "404 Not Found - {{uri}}"
        },
        {
          "expr": "rate(http_server_requests_seconds_count{job=\"tech-test-app\",status=\"500\"}[5m])",
          "legendFormat": "500 Internal Server Error - {{uri}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 32
      }
    },
    {
      "id": 12,
      "title": "HTTP Status Code Distribution",
      "type": "piechart",
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=\"tech-test-app\",status=\"200\"}[5m]))",
          "legendFormat": "200 OK"
        },
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=\"tech-test-app\",status=\"400\"}[5m]))",
          "legendFormat": "400 Bad Request"
        },
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=\"tech-test-app\",status=\"404\"}[5m]))",
          "legendFormat": "404 Not Found"
        },
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=\"tech-test-app\",status=\"500\"}[5m]))",
          "legendFormat": "500 Internal Server Error"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 32
      }
    },
    {
      "id": 13,
      "title": "HTTP Error Rate %",
      "type": "stat",
      "targets": [
        {
          "expr": "clamp_max((sum(rate(http_server_requests_seconds_count{job=\"tech-test-app\",status=~\"4..|5..\"}[5m])) / (sum(rate(http_server_requests_seconds_count{job=\"tech-test-app\"}[5m])) + 0.0001)) * 100, 100)",
          "legendFormat": "HTTP Error Rate %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1},
              {"color": "red", "value": 5}
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 40
      }
    },
    {
      "id": 14,
      "title": "HTTP Success Rate %",
      "type": "stat",
      "targets": [
        {
          "expr": "clamp_max((sum(rate(http_server_requests_seconds_count{job=\"tech-test-app\",status=\"200\"}[5m])) / (sum(rate(http_server_requests_seconds_count{job=\"tech-test-app\"}[5m])) + 0.0001)) * 100, 100)",
          "legendFormat": "HTTP Success Rate %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 95},
              {"color": "green", "value": 99}
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 6,
        "y": 40
      }
    },
    {
      "id": 15,
      "title": "HTTP Response Time (p95)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(http_server_requests_seconds_bucket{job=\"tech-test-app\"}[5m]))",
          "legendFormat": "p95 Response Time"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 0.5},
              {"color": "red", "value": 1}
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 40
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "5s"
} 