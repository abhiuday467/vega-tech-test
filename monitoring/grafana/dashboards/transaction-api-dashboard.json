{
  "id": null,
  "title": "Transaction API Dashboard",
  "tags": [
    "transaction-api",
    "tech-test",
    "spring-boot"
  ],
  "timezone": "browser",
  "panels": [
    {
      "id": 11,
      "title": "HTTP Status Codes Over Time",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(http_server_requests_seconds_count{job=\"tech-test-app\",status=\"200\"}[5m])",
          "legendFormat": "200 OK - {{uri}}"
        },
        {
          "expr": "rate(http_server_requests_seconds_count{job=\"tech-test-app\",status=\"400\"}[5m])",
          "legendFormat": "400 Bad Request - {{uri}}"
        },
        {
          "expr": "rate(http_server_requests_seconds_count{job=\"tech-test-app\",status=\"404\"}[5m])",
          "legendFormat": "404 Not Found - {{uri}}"
        },
        {
          "expr": "rate(http_server_requests_seconds_count{job=\"tech-test-app\",status=\"500\"}[5m])",
          "legendFormat": "500 Internal Server Error - {{uri}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 32
      }
    },
    {
      "id": 12,
      "title": "HTTP Status Code Distribution",
      "type": "piechart",
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=\"tech-test-app\",status=\"200\"}[5m]))",
          "legendFormat": "200 OK"
        },
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=\"tech-test-app\",status=\"400\"}[5m]))",
          "legendFormat": "400 Bad Request"
        },
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=\"tech-test-app\",status=\"404\"}[5m]))",
          "legendFormat": "404 Not Found"
        },
        {
          "expr": "sum(rate(http_server_requests_seconds_count{job=\"tech-test-app\",status=\"500\"}[5m]))",
          "legendFormat": "500 Internal Server Error"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 32
      }
    },
    {
      "id": 13,
      "title": "HTTP Error Rate %",
      "type": "stat",
      "targets": [
        {
          "expr": "clamp_max((sum(rate(http_server_requests_seconds_count{job=\"tech-test-app\",status=~\"4..|5..\"}[5m])) / (sum(rate(http_server_requests_seconds_count{job=\"tech-test-app\"}[5m])) + 0.0001)) * 100, 100)",
          "legendFormat": "HTTP Error Rate %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 1
              },
              {
                "color": "red",
                "value": 5
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 40
      }
    },
    {
      "id": 14,
      "title": "HTTP Success Rate %",
      "type": "stat",
      "targets": [
        {
          "expr": "clamp_max((sum(rate(http_server_requests_seconds_count{job=\"tech-test-app\",status=\"200\"}[5m])) / (sum(rate(http_server_requests_seconds_count{job=\"tech-test-app\"}[5m])) + 0.0001)) * 100, 100)",
          "legendFormat": "HTTP Success Rate %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 95
              },
              {
                "color": "green",
                "value": 99
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 6,
        "y": 40
      }
    },
    {
      "id": 15,
      "title": "HTTP Response Time (p95)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(http_server_requests_seconds_bucket{job=\"tech-test-app\"}[5m]))",
          "legendFormat": "p95 Response Time"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.5
              },
              {
                "color": "red",
                "value": 1
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 40
      }
    },
    {
      "id": 16,
      "title": "HTTP Request Rate by Endpoint",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum by (uri, method) (rate(http_server_requests_seconds_count{job=\"tech-test-app\"}[5m]))",
          "legendFormat": "{{method}} {{uri}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "reqps"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 17,
      "title": "HTTP Error Rate by Endpoint",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum by (uri, method) (rate(http_server_requests_seconds_count{job=\"tech-test-app\",status=~\"4..|5..\"}[5m]))",
          "legendFormat": "{{method}} {{uri}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "reqps"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 18,
      "title": "HTTP Response Time (p50)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(http_server_requests_seconds_bucket{job=\"tech-test-app\"}[5m]))",
          "legendFormat": "p50 Response Time"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "s",
          "decimals": 3
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 19,
      "title": "HTTP Response Time (p99)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.99, rate(http_server_requests_seconds_bucket{job=\"tech-test-app\"}[5m]))",
          "legendFormat": "p99 Response Time"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "s",
          "decimals": 3
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 6,
        "y": 8
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "5s"
}