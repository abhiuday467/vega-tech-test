    {
      "id": 4,
      "title": "Success Rate %",
      "type": "stat",
      "targets": [
        {
          "expr": "clamp_max((1 - (rate(transaction_errors_total{job=\"tech-test-app\"}[5m]) / (rate(transaction_submissions_total{job=\"tech-test-app\"}[5m]) + rate(transaction_retrievals_total{job=\"tech-test-app\"}[5m]) + 0.0001))) * 100, 100)",
          "legendFormat": "Success Rate"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 95
              },
              {
                "color": "green",
                "value": 99
              }
            ]
